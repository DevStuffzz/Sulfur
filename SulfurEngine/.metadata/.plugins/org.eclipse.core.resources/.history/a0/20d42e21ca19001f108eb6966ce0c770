public class PlayerMovement extends Script {

	@Override
	public void start() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void update(float dt) {
		float x = Input.getAxisHorizontal();
		float y = Input.getAxisVertical();
	}

